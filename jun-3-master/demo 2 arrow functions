console.log(sum2());


var sum3= (x,y)=> x+y;
console.log(sum3(10,80))


var test= ()=> {

	console.log('testing');
	console.log('my arrow');
	console.log('testing');
	console.log('testing');

	return 'this is return value';
}


x= test();
console.log('rv' + x);


